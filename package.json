{
  "name": "llm-chat-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "dev:env": "./start-dev.sh",
    "build": "SIMPLE_PDF=true next build --no-lint",
    "build:vercel": "node scripts/vercel-deploy.js && SIMPLE_PDF=true next build --no-lint",
    "ci": "npm ci --legacy-peer-deps",
    "build:simple": "SIMPLE_PDF=true next build --no-lint",
    "build:github": "GITHUB_PAGES=true next build --no-lint && touch out/.nojekyll && node scripts/github-pages-fix.js && echo 'API routes are not supported in static exports, but client-side apps will handle this gracefully'",
    "start": "next start",
    "lint": "next lint",
    "deploy": "SIMPLE_PDF=true next build --no-lint && firebase deploy --only hosting",
    "deploy:all": "SIMPLE_PDF=true next build --no-lint && firebase deploy",
    "deploy:firebase": "./deploy-firebase.sh",
    "firebase:env": "node scripts/set-firebase-env.js",
    "firebase:serve": "firebase serve",
    "emulate": "firebase emulators:start",
    "proxy": "node proxy-server.js",
    "dev:with-proxy": "concurrently \"npm run dev\" \"npm run proxy\"",
    "test:firebase": "node src/utils/firebase-test.js",
    "test:storage": "node src/utils/firebase-storage-test.js",
    "test:auth": "node src/utils/firebase-auth-test.js",
    "test:spreadsheet": "node src/utils/test-spreadsheet-extraction.js",
    "test:all": "npm run test:firebase && npm run test:auth && npm run test:storage",
    "docker:build": "./docker-build.sh",
    "docker:push": "./docker-push.sh",
    "deploy:cloud-run": "./deploy-cloud-run.sh",
    "docker:dev": "docker-compose up",
    "docker:deploy": "npm run docker:build && npm run docker:push && npm run deploy:cloud-run",
    "vercel-build": "./vercel-build.sh",
    "fix-css": "node ./fix-documents-css.js && node ./fix-postcss.js && node ./fix-css-modules.js",
    "fix-font": "node ./fix-next-font.js",
    "fix-firebase": "./fix-firebase-vercel.sh",
    "fix-icons": "node ./fix-react-icons.js",
    "fix-autoprefixer": "node ./fix-autoprefixer.js",
    "fix-reactgrid": "node ./fix-reactgrid.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.2",
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/storage": "^7.0.0",
    "@langchain/openai": "^0.4.4",
    "@silevis/reactgrid": "^4.1.16",
    "@xenova/transformers": "^2.17.2",
    "compute-cosine-similarity": "^1.1.0",
    "csv-parser": "^3.2.0",
    "firebase": "^11.4.0",
    "firebase-functions": "^6.3.2",
    "is-set": "^2.0.2",
    "langchain": "^0.3.19",
    "mammoth": "^1.9.0",
    "next": "15.2.1",
    "next-themes": "0.4.5",
    "openai": "4.86.2",
    "papaparse": "^5.5.2",
    "pdf-parse": "^1.1.1",
    "pdf-parse-debugging-disabled": "^1.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "5.5.0",
    "react-split-pane": "^2.0.3",
    "react-spreadsheet": "^0.9.5",
    "sharp": "^0.33.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@types/node": "20.17.24",
    "@types/papaparse": "^5.3.15",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "autoprefixer": "9.8.8",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "15.2.1",
    "express": "^4.21.2",
    "firebase-admin": "^13.2.0",
    "http-proxy-middleware": "^3.0.3",
    "is-regexp": "2.1.0",
    "lilconfig": "2.0.4",
    "num2fraction": "1.2.2",
    "picocolors": "0.2.1",
    "postcss": "7.0.39",
    "postcss-import": "12.0.1",
    "postcss-load-config": "2.1.2",
    "postcss-nested": "4.2.3",
    "quick-lru": "5.1.1",
    "source-map": "0.6.1",
    "tailwindcss": "3.3.0",
    "typescript": "5.8.2",
    "yaml": "1.10.2"
  }
}
