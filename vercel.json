{
  "version": 2,
  "buildCommand": "node scripts/vercel-deploy.js && SIMPLE_PDF=true npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install --omit=optional --no-package-lock",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1",
    "VERCEL": "true",
    "NEXT_PUBLIC_VERCEL_DEPLOYMENT": "true",
    "SIMPLE_PDF": "true"
  },
  "cleanUrls": true,
  "trailingSlash": false
}